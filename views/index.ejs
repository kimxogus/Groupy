<!DOCTYPE html>
<html>
<head>
    <base href="/"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>Groupy</title>
    <script>
        (function(d, s, id){
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) {return;}
            js = d.createElement(s);
            js.id = id;js.src = "js/main.js";
            fjs.parentNode.insertBefore(js, fjs);
        })(document, 'script', 'main-script')
    </script>
</head>
<body class="pushable" ng-controller="rootCtrl">
    <%- include sidebar.mobile.ejs %>

    <div id="content-wrapper" class="pusher">
        <div id="sidebar" class="ui left thin sidebar inverted labeled icon vertical menu">
            <%- include sidebar.menu.ejs %>
        </div>
        <div id="content" ng-view></div>
        <%- include footer.ejs %>
    </div>
   <div id="fb-root"></div>
</body>
</html>
